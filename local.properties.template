# This file contains machine-specific paths for the Android SDK
# Copy this file to local.properties and update the sdk.dir path to your Android SDK location

# Example for Windows:
# sdk.dir=C:\\Users\\YourUsername\\AppData\\Local\\Android\\Sdk

# Example for macOS:
# sdk.dir=/Users/YourUsername/Library/Android/sdk

# Example for Linux:
# sdk.dir=/home/YourUsername/Android/Sdk

# Replace the path below with your actual Android SDK path
sdk.dir=YOUR_ANDROID_SDK_PATH_HERE
